{
  "name": "partners-sdk-angular",
  "version": "2.2.0",
  "private": true,
  "description": "A simple AngularJS module for Creads Partners API.",
  "scripts": {
    "test": "karma start --single-run",
    "clean": "rm -rf dist/*",
    "prebuild": "check-node-version --node $(cat .nvmrc) && npm run clean",
    "build": "webpack --babel --bail --mode=production && cd packs && npm pack ../ && cd ../",
    "version": "npm run build && git add -A dist packs",
    "prebump": "npm ci --no-audit",
    "bump": "npm version --allow-same-version --no-git-tag-version $VERSION",
    "postbump": "git add package-lock.json package.json"
  },
  "main": "dist/partners-sdk-angular.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:creads/partners-sdk-angular.git"
  },
  "author": "Samuel Vaillant <s.vaillant@creads.org>",
  "contributors": [
    "Samuel Vaillant <s.vaillant@creads.org>"
  ],
  "homepage": "https://github.com/creads/partners-sdk-angular",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "angular": "^1.3.13",
    "angular-mocks": "^1.3.20",
    "babel-loader": "^8.0.5",
    "chai": "^2.1.1",
    "check-node-version": "^3.3.0",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^0.1.10",
    "karma-mocha-reporter": "^1.0.1",
    "mocha": "^2.2.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  },
  "peerDependencies": {
    "angular": ">=1.3.13"
  },
  "dependencies": {}
}
